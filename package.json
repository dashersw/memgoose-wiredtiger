{
  "name": "memgoose-wiredtiger",
  "version": "1.0.4",
  "description": "WiredTiger storage backend for memgoose - high-performance storage engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:wiredtiger": "node scripts/build-wiredtiger.js",
    "install": "npm run build:wiredtiger && node-gyp rebuild",
    "clean": "rm -rf dist build",
    "prepare": "npm run build",
    "test": "node --import tsx --test tests/*.test.ts",
    "test:file": "node --import tsx --test",
    "test:watch": "node --import tsx --test --watch tests/*.test.ts",
    "test:coverage": "node --import tsx --test --experimental-test-coverage tests/*.test.ts",
    "test:coverage:detailed": "c8 --reporter=html --reporter=text node --import tsx --test tests/*.test.ts",
    "test:coverage:html": "npm run test:coverage:detailed && open coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashersw/memgoose-wiredtiger.git"
  },
  "keywords": [
    "memgoose",
    "wiredtiger",
    "storage",
    "database",
    "mongodb",
    "embedded"
  ],
  "author": "Armagan Amcalar <armagan@amcalar.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/dashersw/memgoose-wiredtiger/issues"
  },
  "homepage": "https://github.com/dashersw/memgoose-wiredtiger#readme",
  "peerDependencies": {
    "memgoose": "^1.0.0"
  },
  "dependencies": {
    "node-addon-api": "^8.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "c8": "^10.1.3",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2"
  }
}
